namespace Tables;

table FB_Meta {
  format_type : int32 ;  // format of contents stored in data blob
  is_deleted  : bool ;   // has this data been deleted?
  data_size   : uint64 ; // number of bytes in data blob

  // the data blob. could be any 
  // supported format (flatbufs, arrow, csv, ...)
  data : [ ubyte ] ;
}

root_type FB_Meta ;
