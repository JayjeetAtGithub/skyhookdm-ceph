namespace Tables;

table Table {
  skyhook_version:ubyte;  // skyhook version
  schema_version:ubyte;   // schema version of flexbuffers contained

  table_name : string ;
  schema     : [ string ] ;
  nrows      : uint64 ;
  ncols      : uint64 ;
  layout     : string ;

  RIDs : [ uint64 ] ;
  att0 : [ uint64 ] ;
  att1 : [ float ] ;
}

root_type Table ;
